    "@nestjs/testing": "^8.0.0",
    "@nrwl/angular": "14.1.9",
    "@nrwl/cli": "14.1.9",
    "@nrwl/cypress": "14.1.9",
    "@nrwl/jest": "14.1.9",
    "@nrwl/nest": "14.1.9",
    "@nrwl/node": "14.1.9",
    "@nrwl/nx-cloud": "16.3.0",
    "@nrwl/tao": "14.1.9",
    "@nrwl/workspace": "14.1.9",

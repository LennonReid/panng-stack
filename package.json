    "ts-jest": "27.1.4",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md,ts,html,graphql}": [
      "yarn run format --uncommitted"
    ]
  },
  "prisma": {
    "schema": "libs/api/data-access/src/schema.prisma"
  }
}
